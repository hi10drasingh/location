{
  "name": "my-webpack-project",
  "version": "1.0.0",
  "description": "My webpack project",
  "main": "index.js",
  "scripts": {
    "lint": "npx eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "npx prettier src test --check",
    "prettier:fix": "npm run prettier -- --write",
    "format": "npm run prettier:fix && npm run lint:fix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky install",
    "build": "webpack --mode=production --node-env=production",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "watch": "webpack --watch",
    "serve": "webpack serve"
  },
  "verbose": true,
  "engines": {
    "npm": ">=6.14.17",
    "node": ">=14.19.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hitendrarao/location.git"
  },
  "keywords": [
    "Google",
    "Maps",
    "Places",
    "API",
    "ES6",
    "Javascript",
    "Plugin"
  ],
  "author": "hitendra.rao@droom.in",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hitendrarao/location/issues"
  },
  "homepage": "https://github.com/hitendrarao/location#readme",
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not IE 10"
  ],
  "devDependencies": {
    "@babel/eslint-parser": "^7.18.2",
    "@types/google.maps": "^3.49.1",
    "@types/node": "^17.0.36",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "@webpack-cli/generators": "^2.4.2",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "declaration-bundler-webpack-plugin": "^1.0.3",
    "eslint": "^8.15.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^8.0.0",
    "prettier": "2.6.2",
    "ts-loader": "^9.3.0",
    "typescript": "^4.7.2",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.0",
    "wepack-cli": "0.0.1-security"
  }
}
