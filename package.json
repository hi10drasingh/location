{
	"name": "@droom/location-plugin",
	"version": "1.0.11",
	"description": "Universal Location Plugin",
	"main": "index.js",
	"scripts": {
		"lint": "run-s lint:style lint:script",
		"lint:style": "npx stylelint **/*.{css,scss,sass}",
		"lint:style:fix": "npm run lint:style -- --fix",
		"lint:script": "npx eslint src --ext .js,.jsx,.ts,.tsx --max-warnings=0",
		"lint:script:fix": "npm run lint:script -- --fix",
		"prettier": "npx prettier src test --check",
		"prettier:fix": "npm run prettier -- --write",
		"format": "run-s prettier:fix lint:fix",
		"test": "jest",
		"prepare": "husky install",
		"build:dev": "webpack --mode=development",
		"build:prod": "webpack --mode=production --node-env=production",
		"watch": "webpack --watch",
		"serve": "webpack serve",
		"build:doc": "npx typedoc",
		"type-check": "tsc --noEmit",
		"type-check:watch": "npm run type-check -- --watch",
		"copy-files:lib": "copyfiles -u 1 \"./src/**/*.html\" \"./src/**/*.css\" \"./src/**/*.txt\" \"./src/**/*.eot\" \"./src/**/*.svg\" \"./src/**/*.ttf\" \"./src/**/*.woff\" lib/",
		"copy-files:lib-esm": "copyfiles -u 1 \"./src/**/*.html\" \"./src/**/*.css\" \"./src/**/*.txt\" \"./src/**/*.eot\" \"./src/**/*.svg\" \"./src/**/*.ttf\" \"./src/**/*.woff\" lib-esm/",
		"clean": "npx shx rm -rf _bundles lib lib-esm",
		"tsc": "tsc",
		"tsc:es6": "tsc -m es6 --outDir lib-esm",
		"build": "run-s lint clean tsc copy-files:lib tsc:es6 copy-files:lib-esm build:prod build:doc",
		"prepublishOnly": "npm run build"
	},
	"verbose": true,
	"engines": {
		"npm": ">=6.14.17",
		"node": ">=14.19.2"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/hitendrarao/location.git"
	},
	"keywords": [
		"Universal",
		"Google",
		"Maps",
		"Places",
		"API",
		"ES6",
		"Javascript",
		"Plugin"
	],
	"author": "hitendra.rao@droom.in",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/hitendrarao/location/issues"
	},
	"homepage": "https://github.com/hitendrarao/location#readme",
	"devDependencies": {
		"@babel/cli": "^7.17.10",
		"@babel/core": "^7.18.2",
		"@babel/eslint-parser": "^7.18.2",
		"@babel/plugin-proposal-class-properties": "^7.17.12",
		"@babel/plugin-proposal-object-rest-spread": "^7.18.0",
		"@babel/preset-env": "^7.18.2",
		"@types/jest": "^28.1.1",
		"@types/node": "^17.0.40",
		"@types/webpack": "^5.28.0",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"@webpack-cli/generators": "^2.4.2",
		"babel-loader": "^8.2.5",
		"copyfiles": "^2.4.1",
		"css-loader": "^6.7.1",
		"eslint": "^8.17.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-airbnb-typescript": "^17.0.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-compat": "^4.0.2",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-jsdoc": "^39.3.2",
		"fork-ts-checker-webpack-plugin": "^7.2.11",
		"husky": "^8.0.1",
		"jest": "^28.1.0",
		"lint-staged": "^13.0.0",
		"npm-run-all": "^4.1.5",
		"prettier": "2.6.2",
		"shx": "^0.3.4",
		"style-loader": "^3.3.1",
		"stylelint": "^14.8.5",
		"stylelint-config-standard": "^25.0.0",
		"terser-webpack-plugin": "^5.3.3",
		"ts-jest": "^28.0.4",
		"ts-loader": "^9.3.0",
		"typedoc": "^0.22.17",
		"typedoc-plugin-markdown": "^3.12.1",
		"typedoc-plugin-missing-exports": "^0.22.6",
		"typescript": "^4.7.3",
		"webpack": "^5.73.0",
		"webpack-cli": "^4.9.2",
		"webpack-dev-server": "^4.9.1"
	},
	"dependencies": {
		"@types/google.maps": "^3.49.2"
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": [
			"eslint --fix --max-warnings=0"
		],
		"*.{css,scss,sass}": [
			"stylelint --fix"
		]
	}
}
